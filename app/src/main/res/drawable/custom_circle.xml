<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">
    <!-- 외부 원 모양 -->
    <item>
        <shape
            android:shape="oval">
            <solid android:color="@color/green2" /> <!-- 외부 원의 색상 설정 -->
        </shape>
    </item>
    <!-- 내부 원 모양 (뚤린 부분) -->
    <item android:top="7dp" android:left="7dp" android:right="7dp" android:bottom="7dp">
        <shape
            android:shape="oval">
            <solid android:color="@color/white" /> <!-- 내부 원의 색상 설정 -->
        </shape>
    </item>
</layer-list>